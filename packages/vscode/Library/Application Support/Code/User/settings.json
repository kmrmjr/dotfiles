{
    "vim.vimrc.enable": true,
    "vim.leader": "<space>",
    "vim.vimrc.path": "~/.config/nvim/maps.vim",
    "vim.useSystemClipboard": true,
    "vim.easymotion": true,
    "vim.surround": true,
    "terminal.integrated.inheritEnv": false,
    "editor.suggestSelection": "first",
    "python.languageServer": "Default",
    "editor.formatOnPaste": false,
    "editor.formatOnType": false,
    "workbench.iconTheme": "material-icon-theme",
    "github.gitAuthentication": false,
    "vim.hlsearch": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        // {
        //     "before": ["leader", "s"],
        //     "after": ["leader", "leader", "k"]
        //     // "after": ["leader", "leader", "2", "s"]
        // },
        {
            "before": ["leader", "k"],
            "after": ["leader", "leader", "k"]
        },
        {
            "before": ["leader", "j"],
            "after": ["leader", "leader", "j"]
        },
        {
            "before": ["g", "i"],
            "commands": [
                "editor.action.editor.action.peekImplementationn"
            ]
        },
        {
            "before": ["g", "y"],
            "commands": [
                "editor.action.goToTypeDefinition"
            ]
        },
        {
            "before": ["g", "r"],
            "commands": [
                "editor.action.goToReferences"
            ]
        },
        {
            "before": ["<C-j>"],
            "commands": [
                "editor.action.marker.nextInFiles"
            ]
        },
        {
            "before": ["leader", "r", "n"],
            "commands": [
                "editor.action.rename"
            ]
        },
    ],
    "markdown.extension.preview.autoShowPreviewToSide": true,
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.cursorStyle": "line",
    "editor.insertSpaces": false,
    "editor.lineNumbers": "on",
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "editor.wordWrap": "on",
    "notebook.cellToolbarLocation": {
        "default": "right"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "prettier.tabWidth": 4,
    "prettier.singleQuote": true,
    "terminal.integrated.defaultProfile.osx": "zsh",
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "terminal.integrated.profiles.osx": {
        "/bin/bash (migrated)": {
            "path": "/bin/bash",
            "args": ["-l"]
        }
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "editor.bracketPairColorization.enabled": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.columnSelection": true,
    "multiCommand.commands": [
        {
            "command": "multiCommand.closeExplorerAfterOpenFile",
            "sequence": [
                "list.select",
                "workbench.action.toggleSidebarVisibility"
                // "workbench.files.action.collapseExplorerFolders",
            ]
        },
        {
            "command": "multiCommand.closeExplorerAfterOpenFileInSplitedEditor",
            "sequence": [
                "explorer.openToSide",
                "workbench.action.toggleSidebarVisibility"
                // "workbench.files.action.collapseExplorerFolders",
            ]
        },
        {
            "command": "multiCommand.viewExplorerClosingAllFolder",
            "sequence": [
                "workbench.view.explorer",
                // "workbench.files.action.collapseExplorerFolders",
                "list.focusFirst"
            ]
        }
    ],
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "prettier.jsxSingleQuote": true,
    "prettier.requireConfig": true,
    "eslint.codeActionsOnSave.rules": null,
    "window.zoomLevel": 1,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.fontFamily": "'Ubuntu Mono', Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 13,
    "python.linting.mypyEnabled": true,
    "[python]": {
        "editor.formatOnType": true
    },
    "markdown.marp.enableHtml": true
}
